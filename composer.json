{
    "name": "eyewitness/eye",
    "description": "Eyewitness.io client for Laravel 5 applications",
    "keywords": [
        "laravel 5",
        "queue",
        "cron",
        "monitor",
        "eyewitness"
    ],
    "homepage": "https://github.com/eyewitness/eye",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurence",
            "email": "laurence@eyewitness.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "5.*|6.*",
        "laravel/framework": "~5.0.14|~5.1|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Eyewitness\\Eye\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eyewitness\\Eye\\Test\\": "tests"
        },
        "classmap": ["tests/TestCase.php"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eyewitness\\Eye\\EyeServiceProvider"
            ]
        }
    }
}
